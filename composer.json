    "keywords": [
        "MetaApi",
        "victorycodedev",
        "php-metaapi-sdk"
    ],
    "type": "library",
    "homepage": "https://github.com/victorycodedev/metaapi-cloud-php-sdk",
    "license": "MIT",
    "autoload": {
        "psr-4": {
